<section class="courses" style="padding-top: 120px;">
    <div class="container">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 course-title">
            <h1>My learning</h1>
            <p>Choose from 183,000 online video courses with new additions published every month</p>
        </div>
        <div class="row">
            <% lessons.forEach(function(lessons) { %>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6  course-main-slider ">
                <a id="<%= lessons.course_id %>" href="#" class="slider">
                    <img src="<%= lessons.image %>" alt="">
                    <h3><%= lessons.title %></h3>
                    <h4><%= lessons.description %></h4>
                </a>
            </div>
            <% }); %>
            <div class="col-lg-12 d-flex justify-content-center">
                <nav class="pagination-container">
                    <div class="pagination">
                        <a class="pagination-newer" href="#">PREV</a>
                        <span class="pagination-inner">
                            <a href="#">1</a>
                            <a class="pagination-active" href="#">2</a>
                            <a href="#">3</a>
                            <a href="#">4</a>
                            <a href="#">5</a>
                            <a href="#">6</a>
                        </span>
                        <a class="pagination-older" href="#">NEXT</a>
                    </div>
                </nav>
            </div>
        </div>
    </div>
</section>