<section class="courses" style="padding-top: 82px;">
    <div class="container">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 course-title">
            <h1>A broad selection of courses</h1>
            <p>Choose from 183,000 online video courses with new additions published every month</p>
            <button class="course-button active" data-title="#python" title="Python">
                All courses
            </button>
        </div>
        <div class="row" id="content">

            <% courses.forEach(function(courses) { %>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 course-main-slider ">
                    <a id="<%= courses.id %>" href="/courses/<%= courses.slug %>" class="slider">
                        <img src="<%= courses.image %>" alt="">
                        <h3>
                            <%= courses.name %>
                        </h3>
                        <h4>
                            <%= courses.author %>
                        </h4>
                        <h6>A$<%= courses.price %>
                        </h6>
                    </a>
                </div>
                <% }); %>
        </div>
        <div class="col-lg-12 d-flex justify-content-center">


            <div id="paging">

            </div>

        </div>


    </div>
</section>
<script src="/assets/js/pagination.min.js"></script>
<script src="/assets/js/pagination.js"></script>